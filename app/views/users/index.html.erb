<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>About</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.user_name %></td>
      <td><%= user.about %></td>
      <td><%= link_to 'Edit', edit_user_path(user) %></td>
      <td><%= link_to 'Delete', user, method: :delete, data: {confirm:"are you sure?"} %></td>
    </tr>
    <% end %>
  </tbody>

</table>

<%= link_to 'Add User', new_user_path %><br>
<%= link_to 'See Courses', courses_path %>
